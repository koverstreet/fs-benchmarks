#!/bin/bash

fio --group_reporting					\
    --runtime=120					\
    --size=16G						\
    --name=dio-randrw					\
    --ioengine=libaio					\
    --direct=1						\
    --iodepth=32					\
    --iodepth_batch_complete_min=1			\
    --iodepth_batch_complete_max=64			\
    --rw=randrw						\
    --blocksize=4k					\
    --cpus_allowed=0
