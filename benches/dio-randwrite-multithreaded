#!/bin/bash

fio --output-format=json+ --group_reporting					\
    --runtime=60					\
    --size=8G						\
    --name=dio-randwrite-multithreaded			\
    --ioengine=libaio					\
    --direct=1						\
    --iodepth=64					\
    --rw=randwrite					\
    --blocksize=4k					\
    --numjobs=6						\
    --overwrite=1
