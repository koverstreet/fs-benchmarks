#!/bin/bash

dd if=/dev/zero of=test bs=16M count=1024 oflag=direct 2>/dev/null

#perf record -a -F max -o /root/perf.data --		\
fio --output-format=json+ --group_reporting					\
    --runtime=60					\
    --size=16G						\
    --name=dio-randwrite				\
    --ioengine=libaio					\
    --direct=1						\
    --iodepth=64					\
    --rw=randwrite					\
    --blocksize=4k					\
    --overwrite=1					\
    --filename=test
